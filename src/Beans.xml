<?xml version="1.0" encoding="UTF-8"?>
<!-- Các dòng 5,7,8,11 dưới đây được thêm để thực hiện annotation -->
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>	

 	<bean id="carAuto" class="com.devwind.Car"></bean>
	
	<bean id="engine1" class="com.devwind.Engine">
		<property name="enginePrice" value="10000"></property>
		<property name="engineOrigin" value="GERMANY"></property>
	</bean>
	<bean id="frame1" class="com.devwind.Frame">
		<constructor-arg type="int" value="1000"></constructor-arg>
		<constructor-arg type="java.lang.String" value="CHINA"></constructor-arg>
	</bean>
	<bean id="engine2" class="com.devwind.Engine">
		<property name="enginePrice" value="10000"></property>
		<property name="engineOrigin" value="USA"></property>
	</bean>
	<bean id="frame2" class="com.devwind.Frame">
		<constructor-arg type="int" value="1000"></constructor-arg>
		<constructor-arg type="java.lang.String" value="VIETNAM"></constructor-arg>
	</bean>
  </beans>
